<%- include("partials/header") -%>
<h1>Home</h1>

<p><%= startingContent %></p>






  <% posts.forEach(function(post){ %>
    <div class="item">
      
        <h1> <%= post.title %> </h1>
        <form action="/post" method="post">
            <p> <%= post.content.substring(0, 100) %> ... 
                <a href="http://localhost:3000/post/<%= post.title %> ">Read more</a>
            </p>
        </form>
        
        
    </div>
    <% }) %>
   







<%- include("partials/footer") -%>

